<h1>Listing volunteer_contacts</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Uuid</th>
    <th>Address</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Application date</th>
    <th>Organization</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @volunteer_contacts.each do |volunteer_contact| %>
  <tr>
    <td><%= volunteer_contact.first_name %></td>
    <td><%= volunteer_contact.last_name %></td>
    <td><%= volunteer_contact.uuid %></td>
    <td><%= volunteer_contact.address %></td>
    <td><%= volunteer_contact.phone %></td>
    <td><%= volunteer_contact.email %></td>
    <td><%= volunteer_contact.application_date %></td>
    <td><%= volunteer_contact.organization %></td>
    <td><%= link_to 'Show', volunteer_contact %></td>
    <td><%= link_to 'Edit', edit_volunteer_contact_path(volunteer_contact) %></td>
    <td><%= link_to 'Destroy', volunteer_contact, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Volunteer contact', new_volunteer_contact_path %>
