<div class="row-fluid">
  <div class="span12">
    <%= form_for foster_contact, :url => admin_foster_contacts_path, :validate => true do |f| %>
      <% if foster_contact.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(foster_contact.errors.count, "error") %> prohibited this foster_contact from being saved:</h2>

          <ul>
          <% foster_contact.errors.full_messages.each do |msg| %>
