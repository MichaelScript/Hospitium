<script type="text/javascript">
	$(document).ready(function(){jQuery.jqplot("chart1",[<%= raw(final_status_hash['pie'].to_json) %>],{grid:{drawBorder:!1,drawGridlines:!1,background:"transparent",shadow:!1},seriesColors:"#006DCC,#49AFCD,#5BB75B,#FAA732,#DA4F49,#ff6138,#ffff9d,#beeb9f,#79bd8f,#00a387,#0085cc".split(","),seriesDefaults:{renderer:jQuery.jqplot.PieRenderer,rendererOptions:{diameter:190,sliceMargin:8,showDataLabels:!1}},legend:{show:!0,location:"e"}})});
	$(document).ready(function(){jQuery.jqplot("chart2",[<%= raw(final_species_hash['pie'].to_json) %>],{grid:{drawBorder:!1,drawGridlines:!1,background:"transparent",shadow:!1},seriesColors:"#006DCC,#49AFCD,#5BB75B,#FAA732,#DA4F49,#ff6138,#ffff9d,#beeb9f,#79bd8f,#00a387,#0085cc".split(","),seriesDefaults:{renderer:jQuery.jqplot.PieRenderer,rendererOptions:{diameter:190,sliceMargin:8,showDataLabels:!1}},legend:{show:!0,location:"e"}});$("#chart1").bind("jqplotDataHighlight",function(a,c,d,b){$("span:contains('"+b[0]+"')").css("font-weight","600");$("span:contains('"+
